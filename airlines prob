import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef {
    public static void main(String[] args) throws java.lang.Exception {
        Scanner scanner = new Scanner(System.in);
        int T = scanner.nextInt();
        while (T-- > 0) {
            int X = scanner.nextInt(); 
            int N = scanner.nextInt();

            int requiredPlanes = (int) Math.ceil((double) N / 100);
            int planesToPurchase = Math.max(0, requiredPlanes - X); 

            System.out.println(planesToPurchase);
        }
        scanner.close();
    }
}
