class Solution {
    public int countNonMinimum(int[] nums) {
        // write your code here 
        if(nums.length==0)
        return 0;
        int minimum = nums[0];
        int countmin=0;
        for(int num:nums){
            if(num<minimum)minimum=num;
        }
        for(int num:nums){
            if(num==minimum) countmin++;
        }
        return nums.length-countmin;
        
    }
}
