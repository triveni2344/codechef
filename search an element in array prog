import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner(System.in);
		int n=sc.nextInt();
		int x=sc.nextInt();
		boolean found =false;
		for(int i=0;i<n;i++){
		    int a=sc.nextInt();
		    if(a==x){
		    found=true;
		    break;
		   }
		}
		if(found)
		System.out.println("yes");
		else
		System.out.println("no");
	}
}
