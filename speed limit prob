import java.util.*;
import java.lang.*;
import java.io.*;

class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();

		while (t-- > 0) {
			int a = sc.nextInt();
			int x = sc.nextInt();
			int b = sc.nextInt();
			int y = sc.nextInt();

			// Calculate speeds (or compare products to avoid floating-point issues)
			//double speedAlice = (double) a / x;
			//double speedBob = (double) b / y;

			if (a * y > b * x) {
				System.out.println("Alice");
			} else if (a * y < b * x) {
				System.out.println("Bob");
			} else {
				System.out.println("Equal");
			}
		}
	}
}
