import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner scanner = new Scanner(System.in);
        int X = scanner.nextInt();
        double Y = scanner.nextDouble();

        if (X % 5 == 0 && X + 0.50 <= Y) {
            Y -= (X + 0.50);
        }

        System.out.printf("%.2f\n", Y);
	}
}
